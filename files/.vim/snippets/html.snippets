#html
snippet html
	<!DOCTYPE html>
	<html lang="en">
	<head>
		<meta charset="utf-8" />
	</head>
	<body>
		<div class="container">
			${1}
		</div>
	</body>
	</html>
snippet h1
	<h1>${1}</h1>
snippet h2
	<h2>${1}</h2>
snippet h3
	<h3>${1}</h3>
snippet p
	<p>${1}</p>
snippet div
	<div>${1}</div>
snippet span
	<span>${1}</span>
snippet js
	<script src="${1}" type="text/javascript"></script>
snippet css
	<link href="${1}" media="screen, projection" rel="stylesheet" type="text/css" />
